{
	"name": "easycv",
	"private": true,
	"sideEffects": false,
	"type": "module",
	"scripts": {
		"build": "remix vite:build",
		"dev": "NODE_ENV=development dotenv -- node ./server.js",
		"dev:debug": "NODE_ENV=development dotenv -- node --inspect ./server.js",
		"start": "cross-env NODE_ENV=production node ./server.js",
		"typecheck": "tsc",
		"deploy-production": "vercel --prod",
		"format": "npx @biomejs/biome format --write .",
		"lint": "npx @biomejs/biome check --write .",
		"lint-check": "npx @biomejs/biome check .",
		"sync-db-types": "npx supabase gen types --lang=typescript --project-id rhcnxneqoqidlhhorrvu --schema public > db_types.ts",
		"seed": "dotenv -- tsx app/lib/supabase/seed.ts",
		"stripe-listen": "stripe listen --skip-verify --forward-to localhost:3000/api/webhook",
		"create-update": "node scripts/create-update.js"
	},
